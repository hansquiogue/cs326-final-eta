{
  "name": "euryale",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "lint": "eslint client/ server/"
  },
  "devDependencies": {
    "eslint": "^7.11.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "mongodb": "^3.6.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "consistent-return": "error",
      "no-var": "error",
      "eqeqeq": "error",
      "semi": [
        "error",
        "always"
      ],
      "curly": [
        "warn",
        "all"
      ],
      "prefer-const": [
        "warn",
        {}
      ]
    }
  }
}
